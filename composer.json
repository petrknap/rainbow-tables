{
    "name": "petrknap/rainbow-tables",
    "description": "Implementation of rainbow tables",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "dev@petrknap.cz",
            "homepage": "http://petrknap.cz",
            "role": "Developer"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "petrknap/utils": "2015.01.20"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "PetrKnap\\RainbowTables\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "crc32-generate": "PetrKnap\\RainbowTables\\Plugin\\Crc32\\Crc32Scripts::generate",
        "crc32-find": "PetrKnap\\RainbowTables\\Plugin\\Crc32\\Crc32Scripts::find",
        "md5-generate": "PetrKnap\\RainbowTables\\Plugin\\Md5\\Md5Scripts::generate",
        "md5-find": "PetrKnap\\RainbowTables\\Plugin\\Md5\\Md5Scripts::find"
    }
}
